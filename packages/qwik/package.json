{
  "name": "qwik",
  "version": "0.0.9",
  "licence": "MIT",
  "description": "Blank Qwik starter app. Vite.js tooling. Cloudflare Pages. Prettier code formatter. Tailwind CSS framework.",
  "scripts": {
    "build": "pnpm run typecheck && npm run build.client && npm run build.ssr",
    "build.client": "vite build",
    "build.ssr": "vite build --ssr src/entry.cloudflare.tsx",
    "dev": "vite",
    "dev.debug": "node --inspect-brk node_modules/vite/bin/vite.js --mode ssr",
    "dev.ssr": "node --inspect node_modules/vite/bin/vite.js --mode ssr",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "serve": "wrangler pages dev ./dist",
    "start": "pnpm run dev",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.0.100",
    "@types/eslint": "8.4.5",
    "@types/node": "18.7.4",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "autoprefixer": "10.4.8",
    "eslint": "8.22.0",
    "eslint-plugin-qwik": "0.0.100",
    "node-fetch": "3.2.10",
    "postcss": "8.4.16",
    "prettier": "2.7.1",
    "tailwindcss": "3.1.8",
    "typescript": "4.7.4",
    "vite": "3.0.7",
    "wrangler": "2.0.25"
  },
  "engines": {
    "node": ">=14"
  },
  "homepage": "https://qwik.builder.io/",
  "private": false
}
